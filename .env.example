# 量化择时指标应用 - 环境变量示例
# 复制此文件为 .env 并填写实际值

# 应用配置
APP_NAME=量化择时指标应用
APP_ENV=development
APP_DEBUG=True
SECRET_KEY=your-secret-key-here

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=5000

# AI配置
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# 数据库配置
DATABASE_TYPE=file
DATABASE_FILE_PATH=data/application_data.json

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/app.log

# 缓存配置
CACHE_ENABLED=True
CACHE_TTL_MINUTES=30